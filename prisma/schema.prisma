generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(cuid())
  name     String  @db.VarChar(32)
  email    String  @unique
  phone    String?
  password String

  @@map("users")
}
